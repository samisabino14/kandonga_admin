        <script>

            function initMap() {
                
                // The location of Luanda
                var luanda = {
                    
                    lat: -8.8383,
                    lng: 13.2344
                };

                // The map, centered at Luanda
                
                var map = new google.maps.Map(
                
                    document.getElementById('map'), {
                    
                        zoom: 4,
                        center: luanda
                    }
                );

                // The marker, positioned at Luanda
                var marker = new google.maps.Marker({
                    
                    position: luanda,
                    map: map
                });
            }
            
